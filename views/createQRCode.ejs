<div class="row">
	<div class="col-12">
		<div class="card shadow-sm mx-md-4">
			<div class="card-body">
				<div class="input-group mb-3">
					<input type="text" class="form-control" id="txtCode">
					<div class="input-group-append">
						<button class="btn btn-outline-secondary" type="button" onclick="addCode()"><i
								class="fa-solid fa-plus fa-fw"></i>Ekle</button>
					</div>
				</div>
				<button class="btn mb-2 btn-outline-secondary" type="button" onclick="print()"><i
						class="fa-solid fa-print fa-fw"></i> Yazdır</button>
				<hr>
				<h5>Seçili QRCode'lar</h5>
				<table class="table table-striped table-bordered">
					<thead>
						<tr>
							<th>#</th>
							<th>QR Code</th>
							<th>Ürün Adı</th>
							<th>Stok Miktarı</th>
							<th>Sil</th>
						</tr>
					</thead>
					<tbody id="codes">
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>